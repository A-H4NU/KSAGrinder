<Window Style="{DynamicResource WindowStyle}" x:Class="KSAGrinder.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KSAGrinder"
        xmlns:statics="clr-namespace:KSAGrinder.Statics"
        mc:Ignorable="d"
        Title="KSAGrinder" Height="768" Width="1366">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="ResourceDictionaries/WindowStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <Grid>
        <Frame NavigationUIVisibility="Hidden" statics:DisableNavigation.Disable="True" Name="Main"/>
    </Grid>
</Window>
