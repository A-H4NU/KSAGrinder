<Window
    x:Class="KSAGrinder.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:KSAGrinder"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:statics="clr-namespace:KSAGrinder.Statics"
    Title="KSAGrinder"
    Width="1366"
    Height="768"
    MinWidth="1366"
    MinHeight="768"
    SizeToContent="Width"
    Style="{DynamicResource WindowStyle}"
    mc:Ignorable="d">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="ResourceDictionaries/WindowStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <Grid>
        <Frame
            Name="Main"
            statics:DisableNavigation.Disable="True"
            NavigationUIVisibility="Hidden" />
    </Grid>
</Window>
